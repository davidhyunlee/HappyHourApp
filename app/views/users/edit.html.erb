<%= form_for @user do |f| %>

		<%= f.label :first_name %><%= f.text_field :first_name %>
		<%= f.label :last_name %><%= f.text_field :last_name %>
		<%= f.label :username %><%= f.text_field :username %>
		<%= f.label :location %><%= f.text_field :location %>
		<%= f.label :hometown %><%= f.text_field :hometown %>
		<%= f.label :nickname %><%= f.text_field :nickname %>
		<%= f.label :headline %><%= f.text_field :headline %>
		<%= f.label :website %><%= f.text_field :website %>
		<%= f.label :password %><%= f.text_field :password %>
		<%= f.label :password_confirmation %><%= f.text_field :password_confirmation %>

	<%= f.submit "Update Profile" %>

<% end %>


